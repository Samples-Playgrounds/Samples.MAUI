<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <packageSources>
    <!--To inherit the global NuGet package sources remove the <clear/> line below -->
    <clear />
    <!-- 
      https://docs.microsoft.com/en-us/nuget/reference/nuget-config-file#nugetconfig-environment-variable-support
    -->
    <add key="nuget-local" value="%HOME%/nuget-local/" />
    <add key="nuget" value="https://api.nuget.org/v3/index.json" />
  </packageSources>
</configuration>
